# Environment Configuration Example
# Copy this file to .env and fill in your values
# NEVER commit .env to version control

# ===================================
# LLM API Keys
# ===================================
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
COHERE_API_KEY=your_cohere_api_key_here

# ===================================
# Application Settings
# ===================================
AGENT_ENV=development
AGENT_DEBUG=true
AGENT_LOG_LEVEL=INFO

# ===================================
# Security
# ===================================
SECRET_KEY=your_secret_key_minimum_32_characters_for_development
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# ===================================
# Database Configuration
# ===================================
DATABASE_URL=postgresql://agent_user:agent_pass@localhost:5432/agents
DATABASE_POOL_SIZE=10
DATABASE_MAX_OVERFLOW=20

# ===================================
# Redis Configuration
# ===================================
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_SSL=false

# ===================================
# API Server Configuration
# ===================================
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=1
API_RELOAD=true

# ===================================
# Agent Configuration
# ===================================
AGENT_DEFAULT_MODEL=gpt-4
AGENT_DEFAULT_TEMPERATURE=0.7
AGENT_DEFAULT_MAX_TOKENS=4096
AGENT_MAX_CONCURRENT=10

# ===================================
# Memory Configuration
# ===================================
MEMORY_BACKEND=chromadb
MEMORY_PERSIST_DIR=./data/memory
MEMORY_COLLECTION_NAME=agent_memory

# ===================================
# Vector Store Configuration
# ===================================
# ChromaDB
CHROMA_HOST=localhost
CHROMA_PORT=8000

# Pinecone
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=your_pinecone_environment

# Weaviate
WEAVIATE_URL=http://localhost:8080
WEAVIATE_API_KEY=your_weaviate_api_key_here

# ===================================
# Observability Configuration
# ===================================
# Metrics
PROMETHEUS_PORT=9090
ENABLE_METRICS=true

# Tracing
ENABLE_TRACING=true
JAEGER_ENDPOINT=http://localhost:14268/api/traces
TRACING_SAMPLING_RATE=0.1

# Logging
LOG_FORMAT=json
LOG_FILE=./logs/agent.log

# ===================================
# Cloud Provider Configuration
# ===================================
# AWS
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
AWS_S3_BUCKET=your_s3_bucket_name

# GCP
GCP_PROJECT_ID=your_gcp_project_id
GCP_SERVICE_ACCOUNT_KEY=path/to/service-account-key.json

# Azure
AZURE_SUBSCRIPTION_ID=your_azure_subscription_id
AZURE_RESOURCE_GROUP=your_resource_group
AZURE_STORAGE_ACCOUNT=your_storage_account

# ===================================
# External Services
# ===================================
# Search APIs
SERP_API_KEY=your_serp_api_key
BRAVE_API_KEY=your_brave_api_key

# News APIs
NEWS_API_KEY=your_news_api_key

# Weather APIs
OPENWEATHER_API_KEY=your_openweather_api_key

# ===================================
# Monitoring and Alerting
# ===================================
GRAFANA_URL=http://localhost:3000
GRAFANA_API_KEY=your_grafana_api_key

# Slack
SLACK_WEBHOOK_URL=your_slack_webhook_url
SLACK_CHANNEL=#alerts

# Email
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_email_password
ALERT_EMAIL=alerts@yourdomain.com

# ===================================
# Rate Limiting
# ===================================
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW_MINUTES=1

# ===================================
# CORS Configuration
# ===================================
CORS_ORIGINS=http://localhost:3000,http://localhost:8080
CORS_ALLOW_CREDENTIALS=true

# ===================================
# Development Tools
# ===================================
# LangSmith
LANGSMITH_API_KEY=your_langsmith_api_key
LANGSMITH_PROJECT=your_project_name
LANGSMITH_TRACING=true

# Weights & Biases
WANDB_API_KEY=your_wandb_api_key
WANDB_PROJECT=master-agentic-ai
WANDB_ENTITY=your_wandb_entity

# ===================================
# Testing Configuration
# ===================================
TEST_DATABASE_URL=postgresql://test_user:test_pass@localhost:5432/test_agents
TEST_REDIS_URL=redis://localhost:6379/1
TEST_OPENAI_API_KEY=sk-test-key

# ===================================
# Feature Flags
# ===================================
ENABLE_EXPERIMENTAL_FEATURES=false
ENABLE_TELEMETRY=true
ENABLE_CACHE=true
ENABLE_COMPRESSION=true